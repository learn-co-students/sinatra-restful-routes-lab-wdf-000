<!DOCTYPE html>
<html>
  <head>
    <title>Index<title>
  </head>
  <body>
    <% @recipes.each do |recipe| %>
      <h4><%= recipe.name %></h4>
      <p><%= recipe.ingredients %></p>
      <p><%= recipe.cook_time %></p>
      <a href="/recipes/<%= recipe.id %>">see more</a>
      <br>
    <% end %>
  </body>
</html>
